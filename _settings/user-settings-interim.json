{
    "window.zoomLevel": 0,
    "editor.fontFamily": "Intel One Mono",
    "editor.fontSize": 16,
    "editor.fontWeight": 100,
    "editor.lineHeight": 1.2,
    "debug.console.fontFamily": "IBM3270",
    "debug.console.fontSize": 14,
    "debug.console.lineHeight": 15,
    "editor.codeLensFontFamily": "IntelOneMono",
    "terminal.integrated.fontFamily": "IBM3270",
    "terminal.integrated.fontSize": 14,
    "terminal.integrated.fontWeight": 100,
    "[Log]": {
        "editor.fontFamily": "IBM3270",
        "editor.fontSize": 14,
        "editor.lineHeight": 15
    },
    "[javascript]": {
        "editor.defaultFormatter": "dbaeumer.vscode-eslint",
        "editor.maxTokenizationLineLength": 2500
    },
    
    "[svelte]": {
        "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    },
    "code-runner.enableAppInsights": false,
    "CodeGPT.model": "gpt-4",
    "editor.acceptSuggestionOnCommitCharacter": true,
    "editor.acceptSuggestionOnEnter": "smart",
    "editor.autoClosingDelete": "auto",
    "editor.autoIndent": "advanced",
    "editor.bracketPairColorization.enabled": false,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "editor.cursorStyle": "block",
    "editor.cursorSurroundingLines": 4,
    "editor.cursorSurroundingLinesStyle": "all",
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    "editor.dragAndDrop": false,
    "editor.experimental.asyncTokenization": true,
    "editor.experimentalWhitespaceRendering": "font",
    "editor.folding": false,
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.formatOnType": false,
    "editor.glyphMargin": false,
    "editor.guides.bracketPairs": true,
    "editor.guides.highlightActiveBracketPair": true,
    "editor.guides.indentation": true,
    "editor.hover.above": false,
    "editor.hover.delay": 100,
    "editor.hover.sticky": false,
    "editor.inlineSuggest.enabled": true,
    "editor.inlayHints.fontFamily": "Source Code Pro",
    "editor.inlayHints.fontSize": 12,
    "editor.letterSpacing": 0,
    "editor.lineNumbers": "off",
    "editor.links": false,
    "editor.minimap.autohide": false,
    "editor.minimap.maxColumn": 80,
    "editor.minimap.scale": 1,
    "editor.minimap.showSlider": "always",
    "editor.quickSuggestionsDelay": 1,
    "editor.renderLineHighlight": "all",
    "editor.renderWhitespace": "selection",
    "editor.roundedSelection": true,
    "editor.scrollbar.horizontal": "auto",
    "editor.scrollbar.horizontalScrollbarSize": 8,
    "editor.scrollbar.vertical": "visible",
    "editor.scrollbar.verticalScrollbarSize": 18,
    "editor.scrollPredominantAxis": true,
    "editor.semanticHighlighting.enabled": true,
    "editor.semanticTokenColorCustomizations": {
        "[Oceanic Next]": {
            "enabled": true
        }
    },
    "editor.showFoldingControls": "never",
    "editor.smoothScrolling": false,
    "editor.suggestFontSize": 12,
    "editor.suggestLineHeight": 12,
    "editor.suggest.matchOnWordStartOnly": false,
    "editor.suggest.preview": true,
    "editor.tabCompletion": "on",
    "editor.tokenColorCustomizations": {
        "[Oceanic Next (Sublime)]": {
            "textMateRules": [
                {"scope": "keyword",
                    "settings": {"foreground": "#050063"}
                },
                {"scope": "keyword.control",
                    "settings": { "foreground": "#ae00ff"}
                },
                {"scope": "storage.type",
                    "settings": { "foreground": "#ff1100"}
                },
                {"scope": "storage.modifier",
                    "settings": { "foreground": "#ff5500"}
                },
                {"scope": "string.quoted",
                    "settings": { "foreground": "#f8ffc5"}
                },
                {"scope": "string.quoted.double",
                    "settings": { "foreground": "#f8ffc5"}
                },
                {"scope": "string.quoted.single",
                    "settings": { "foreground": "#f8ffc5"}
                },
                {"scope": "string.quoted.triple",
                    "settings": { "foreground": "#f8ffc5"}
                },
                {"scope": "string.quoted.other",
                    "settings": { "foreground": "#f8ffc5"}
                },
                {"scope": "string.template",
                    "settings": { "foreground": "#f8ffc5"}
                },
                {"scope": "string.unquoted",
                    "settings": { "foreground": "#f8ffc5"}
                },
                {"scope": "string.interpolated",
                    "settings": { "foreground": "#f8ffc5"}
                },
                {"scope": "string.regexp",
                    "settings": { "foreground": "#f8ffc5"}
                },
                {"scope": "string.escape",
                    "settings": { "foreground": "#f8ffc5"}
                },
                {"scope": "string",
                    "settings": { "foreground": "#f8ffc5"}
                },
                {"scope": "constant.numeric",
                    "settings": { "foreground": "#ff00ea"}
                },
                {"scope": "constant.language",
                    "settings": { "foreground": "#ff00ea"}
                },
                {"scope": "constant.character",
                    "settings": { "foreground": "#ff00ea"}
                },
                {"scope": "constant.escape",
                    "settings": { "foreground": "#ff00ea"}
                },
                {"scope": "constant.other",
                    "settings": { "foreground": "#ff00ea"}
                },
                {"scope": "variable",
                    "settings": { "foreground": "#ff00ea"}
                },
                {"scope": "variable.parameter",
                    "settings": { "foreground": "#ffea00"}
                },
                {"scope": "variable.other",
                    "settings": { "foreground": "#ff00ee"}
                },
                {
                    "scope": "entity.name.class",
                    "settings": {
                        "foreground": "#ff7700"
                    },
                },
                {
                    "scope": "entity.name.function",
                    "settings": {
                        "fontStyle": "bold underline",
                        "foreground": "#ff00ff"
                    },
                },
                {
                    "scope": [
                        "entity.name.method", 
                        "entity.name.section", 
                        "entity.name.selector", 
                        "entity.name.tag", 
                        "entity.name.type",
                        ],
                    "settings": {
                        "foreground": "#4e4e4e"
                    },
                },
                {"scope": "entity.name.function.member",
                "settings": {
                    "foreground": "#00ffcc",
                    "fontStyle": "bold"
                    }
                }
                
            ]
        },
        "[Material Theme Darker High Contrast]": {
            "textMateRules": [
                {"scope": "keyword.control",
                    "settings": { "foreground": "#ff00ea"}
                },
                {"scope": "storage.type",
                    "settings": { "foreground": "#ff0000"}
                },
                {"scope": "string.quoted",
                    "settings": { "foreground": "#f8ffc5"}
                },
                
            ]
        }
        
    },
    "editor.unfoldOnClickAfterEndOfLine": true,
    "eslint.debug": false,
    "eslint.enable": true,
    "eslint.format.enable": true,
    "eslint.ignoreUntitled": true,
    "eslint.lintTask.enable": true,
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact",
        "svelte",
        "json",
        "jsonc",
        "html"
    ],
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.enableUndo": false,
    "explorer.fileNesting.enabled": true,
    "explorer.sortOrder": "foldersNestsFiles",
    "files.associations": {
        "*.js": "javascript",
        "*.json": "jsonc"
    },
    "git.autofetch": true,
    "git.confirmSync": false,
    "github.copilot-labs.advanced": {
        "enabled": true,
        "showLenses": true,
        "showStatus": true
    },
    "github.copilot-labs.showBrushesLenses": false,
    "github.copilot-labs.showTestGenerationLenses": false,
    "github.copilot.advanced": {
        "enabled": true,
        "showLenses": true,
        "showStatus": true
    },
    "github.copilot.enable": {
        "*": true,
        "javascript": true,
        "json": true,
        "markdown": false,
        "plaintext": true,
        "scminput": false,
        "typescript": true,
        "svelte": true,
    },
    "gitlens.showWelcomeOnInstall": false,
    "gitlens.showWhatsNewAfterUpgrades": false,
    "npm.packageManager": "npm",
    "platformio-ide.activateProjectOnTextEditorChange": true,
    "platformio-ide.autoPreloadEnvTasks": true,
    "platformio-ide.customPATH": "",
    "platformio-ide.disablePIOHomeStartup": true,
    "platformio-ide.forceUploadAndMonitor": true,
    "platformio-ide.useDevelopmentPIOCore": true,
    "problems.decorations.enabled": true,
    "pros.showWelcomeOnStartup": false,
    "pros.useGoogleAnalytics": false,
    "redhat.telemetry.enabled": false,
    "remote.SSH.remotePlatform": {
        "milnet": "linux",
        "n0.tel": "linux",
        "raid": "linux",
        "scotty": "linux"
    },
    "rpcServer.showStartupMessage": false,
    "scm.alwaysShowActions": true,
    "scm.alwaysShowRepositories": true,
    "security.workspace.trust.enabled": false,
    "security.workspace.trust.startupPrompt": "always",
    "settingsSync.ignoredSettings": [
        "editor.fontFamily",
        "debug.console.fontFamily",
        "terminal.integrated.fontFamily"
    ],
    "svelte.enable-ts-plugin": true,
    "telemetry.telemetryLevel": "off",
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.customGlyphs": false,
    "terminal.integrated.defaultProfile.windows": "PowerShell",
    "terminal.integrated.enableBell": true,
    "terminal.integrated.gpuAcceleration": "on",
    "thunder-client.defaultURL": "https://n0.tel/api/",
    "thunder-client.httpVersion": "HTTP/2",
    "update.showReleaseNotes": false,
    "vsicons.dontShowNewVersionMessage": true,
    "window.title": "${rootNameShort}/${activeFolderMedium}/${activeEditorShort} ${dirty}",
    "workbench.colorCustomizations": {
        "editor.background": "#000000",
        "editor.foreground": "#ffffff",
        "editorWhitespace.foreground": "#686868",
        "errorForeground": "#ff0000",
        "errorLens.errorBackground": "#ff000097",
        "errorLens.errorMessageBackground": "#00000088",
        "errorLens.warningBackground": "#ffb300ca",
        "minimap.background": "#0000002d",
        "minimap.errorHighlight": "#ff0000",
        "minimap.warningHighlight": "#fffb00",
        "statusBar.debuggingBorder": "#ff0000",
        "statusBar.debuggingForeground": "#ffda08",
        "terminal.ansiBlack": "#000000",
        "terminal.background": "#000000",
        "terminal.cursorBlinking": true,
        "terminal.cursorStyle": "block",
        "terminal.foreground": "#ffffff",
        "terminal.warningBackground": "#fffb00",
        "terminalCursor.background": "#ffffff",
    },
    "workbench.colorTheme": "Oceanic Next (Sublime)",
    "workbench.commandPalette.experimental.suggestCommands": true,
    "workbench.editor.labelFormat": "short",
    "workbench.editor.tabSizing": "shrink",
    "workbench.editor.splitOnDragAndDrop": false,
    "workbench.enableExperiments": true,
    "workbench.fontAliasing": "none",
    "workbench.layoutControl.enabled": true,
    "workbench.settings.enableNaturalLanguageSearch": false,
    "workbench.settings.openDefaultSettings": false,
    "workbench.startupEditor": "none",
    "workbench.tree.indent": 8,
    "workbench.welcomePage.walkthroughs.openOnInstall": false
}